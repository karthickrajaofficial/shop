{
    "version": 2,
    "routes": [
      { "src": "/api/(.*)", "dest": "https://jewelbyshree.vercel.app/api/$1" },
      { "handle": "filesystem", "src": "/frontend/(.*)", "dest": "/frontend/$1" },
      { "src": "/(.*)", "dest": "/frontend/index.html" }
    ],
    "env": {
      "VITE_API_BASE_URL": "https://jewelbyshree.vercel.app"
    }
  }
  