{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/api/(.*)", "dest": "/frontend/index.js" },
      { "src": "/(.*)", "dest": "/frontend/$1" }
    ]
  }
  